{
  "name": "oinkerui",
  "version": "1.0.0",
  "description": "An LLM-assisted development workbench that provides an interactive chat interface for building software projects with AI assistance",
  "main": "backend/src/index.js",
  "type": "commonjs",
  "directories": {
    "doc": "docs",
    "man": "man",
    "test": "tests"
  },
  "scripts": {
    "dev": "bash scripts/dev.sh",
    "dev:frontend": "bash scripts/dev-frontend.sh",
    "dev:backend": "bash scripts/dev-backend.sh",
    "dev:python": "bash scripts/dev-python.sh",
    "build": "bash scripts/build.sh",
    "setup": "bash scripts/setup.sh",
    "test": "bash scripts/test.sh",
    "test:watch": "jest --watch",
    "lint": "bash scripts/lint.sh",
    "lint:fix": "eslint backend/src --fix",
    "format": "bash scripts/format.sh",
    "clean": "bash scripts/clean.sh",
    "start": "node backend/src/index.js"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.0",
    "@fastify/static": "^7.0.0",
    "axios": "^1.6.0",
    "date-fns": "^3.3.0",
    "dotenv": "^17.2.3",
    "fastify": "^4.26.0",
    "simple-git": "^3.22.0",
    "slugify": "^1.6.0",
    "tiktoken": "^1.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "jest": "^30.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffn0rD/oinkerui.git"
  },
  "keywords": [
    "llm",
    "ai",
